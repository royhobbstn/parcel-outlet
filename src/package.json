{
  "name": "parcel-outlet",
  "version": "0.1.1",
  "private": true,
  "homepage": "https://www.parcel-outlet.com",
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "mapbox-gl": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-mapbox-gl": "^4.8.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://www.parcel-outlet.com --delete --profile parcel-outlet",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id=E2X21IFEOVH42V --paths / --profile=parcel-outlet",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
